    <property name="styleSheet">
     <string notr="true">/* Base Button Style */
QPushButton {
  background-color: rgb(173, 115, 86);
/* Royal Purple */
    color: white;            /* Gold Text */
    border: none;
    border-radius: 20px;        /* Rounded Corners */
    padding: 10px 20px;
    font-size: 16px;
    font-weight: bold;
    transition: all 0.3s ease-in-out;
}

/* Hover Effect */
QPushButton:hover {
    background-color: #8A2BE2;  /* Brighter Purple */
    color: #FFFACD;             /* Lemon Chiffon */
    border: 1px solid #FFD700;  /* Gold Border */
}

/* Pressed Effect */
QPushButton:pressed {
    background-color: rgb(255, 164, 89);/* Indigo */
    color: #D4AF37;             /* Metallic Gold */
    font-size: 15px;            /* Slight Shrink */
    padding: 11px 21px;         /* Compensates for Shrink */
}

/* Focus Effect */
QPushButton:focus {
    outline: none;
    border: 2px solid #FFD700;
}

/* Accent Button Variant */
QPushButton.accent {
    background-color: #FFD700;  /* Gold */
    color: #4B0082;             /* Deep Purple */
}

QPushButton.accent:hover {
    background-color: #F0E68C;  /* Khaki */
    color: #4B0082;
    border: 1px solid #D4AF37;
}

QPushButton.accent:pressed {
    background-color: #D4AF37;  /* Metallic Gold */
    color: #4B0082;
    font-size: 15px;
    padding: 11px 21px;
}
</string>
