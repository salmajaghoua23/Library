         <string notr="true">/* Base Button Style */
QPushButton {
    background-color: rgb(35, 170, 44);  /* Royal Purple */
    color: white;            /* Gold Text */
    border: none;
    border-radius: 20px;        /* Rounded Corners */
    padding: 10px 20px;
    font-size: 16px;
    font-weight: bold;
    transition: all 0.3s ease-in-out;
}

/* Hover Effect */
QPushButton:hover {
    background-color: rgb(179, 126, 82); /* Brighter Purple */
    color: #FFFACD;             /* Lemon Chiffon */
    border: 1px solid #FFD700;  /* Gold Border */
}

/* Pressed Effect */
QPushButton:pressed {
    background-color: #4B0082;  /* Indigo */
    color: #D4AF37;             /* Metallic Gold */
    font-size: 15px;            /* Slight Shrink */
    padding: 11px 21px;         /* Compensates for Shrink */
}

/* Focus Effect */
QPushButton:focus {
    outline: none;
    border: 2px solid #FFD700;
}

